    "            return True\n",
    "            \n",
    "        except Exception as e:\n",
    "            logger.error(f\"Failed to initialize trading components: {e}\")\n",
    "            return False\n",
    "    \n",
    "    def get_current_market_data(self):\n",
    "        \"\"\"\n",
    "        Fetch current market data\n",
    "        \"\"\"\n",
    "        try:\n",
    "            import yfinance as yf\n",
    "            import ta\n",
    "            \n",
    "            # Get current Bitcoin data\n",
    "            btc = yf.Ticker(\"BTC-USD\")\n",
    "            hist = btc.history(period=\"30d\")\n",
    "            info = btc.info\n",
    "            \n",
    "            if hist.empty:\n",
    "                raise Exception(\"No market data available\")\n",
    "            \n",
    "            current_price = info.get('regularMarketPrice', hist['Close'].iloc[-1])\n",
    "            \n",
    "            # Calculate ATR\n",
    "            atr_14 = ta.volatility.average_true_range(\n",
    "                hist['High'], hist['Low'], hist['Close'], window=14\n",
    "            ).iloc[-1]\n",
    "            \n",
    "            # Calculate RSI\n",
    "            rsi_14 = ta.momentum.rsi(hist['Close'], window=14).iloc[-1]\n",
    "            \n",
    "            # Price change\n",
    "            pct_change_1d = ((current_price - hist['Close'].iloc[-2]) / hist['Close'].iloc[-2]) * 100\n",
    "            \n",
    "            return {\n",
    "                'timestamp': datetime.now(),\n",
    "                'price': current_price,\n",
    "                'atr_14': atr_14,\n",
    "                'rsi_14': rsi_14,\n",
    "                'pct_change_1d': pct_change_1d,\n",
    "                'volume': hist['Volume'].iloc[-1]\n",
    "            }\n",
    "            \n",
    "        except Exception as e:\n",
    "            logger.error(f\"Market data error: {e}\")\n",
    "            return None\n",
    "    \n",
    "    def should_execute_dca(self, current_data, last_price):\n",
    "        \"\"\"\n",
    "        Check if DCA should be executed\n",
    "        \"\"\"\n",
    "        if not self.config['enable_dca']:\n",
    "            return False, \"DCA disabled\"\n",
    "            \n",
    "        current_price = current_data['price']\n",
    "        \n",
    "        # Check budget\n",
    "        position_size = self.config['budget'] * (self.config['position_size_pct'] / 100)\n",
    "        if self.portfolio['USD'] < position_size:\n",
    "            return False, \"Insufficient funds\"\n",
    "        \n",
    "        # Price drop trigger\n",
    "        if last_price:\n",
    "            price_change = ((current_price - last_price) / last_price) * 100\n",
    "            if price_change <= -self.config['dca_percentage']:\n",
    "                return True, f\"Price drop: {price_change:.2f}%\"\n",
    "        \n",
    "        # RSI oversold\n",
    "        if current_data.get('rsi_14', 50) <= 30:\n",
    "            return True, f\"RSI oversold: {current_data['rsi_14']:.1f}\"\n",
    "            \n",
    "        return False, \"No triggers met\"\n",
    "    \n",
    "    def execute_dca_buy(self, current_data, reason):\n",
    "        \"\"\"\n",
    "        Execute DCA buy order\n",
    "        \"\"\"\n",
    "        try:\n",
    "            current_price = current_data['price']\n",
    "            position_size = self.config['budget'] * (self.config['position_size_pct'] / 100)\n",
    "            btc_amount = position_size / current_price\n",
    "            \n",
    "            # Execute paper trade\n",
    "            if self.portfolio['USD'] >= position_size:\n",
    "                self.portfolio['USD'] -= position_size\n",
    "                self.portfolio['BTC'] += btc_amount\n",
    "                \n",
    "                trade = {\n",
    "                    'timestamp': current_data['timestamp'],\n",
    "                    'type': 'DCA_BUY',\n",
    "                    'price': current_price,\n",
    "                    'usd_amount': position_size,\n",
    "                    'btc_amount': btc_amount,\n",
    "                    'trigger_reason': reason,\n",
    "                    'atr': current_data.get('atr_14', 0),\n",
    "                    'rsi': current_data.get('rsi_14', 50)\n",
    "                }\n",
    "                \n",
    "                self.trades.append(trade)\n",
    "                self.bot_state['trades_today'] += 1\n",
    "                \n",
    "                logger.info(f\"DCA buy executed: ${position_size:.2f} at ${current_price:.2f}\")\n",
    "                \n",
    "                # Send WhatsApp alert\n",
    "                self.whatsapp.send_trade_alert(\n",
    "                    trade_type=\"DCA_BUY\",\n",
    "                    price=current_price,\n",
    "                    amount=position_size,\n",
    "                    reason=reason\n",
    "                )\n",
    "                \n",
    "                return trade\n",
    "            else:\n",
    "                logger.warning(\"Insufficient USD balance for DCA buy\")\n",
    "                return None\n",
    "                \n",
    "        except Exception as e:\n",
    "            logger.error(f\"DCA buy error: {e}\")\n",
    "            return None\n",
    "    \n",
    "    def execute_trading_cycle(self):\n",
    "        \"\"\"\n",
    "        Execute one complete trading cycle\n",
    "        \"\"\"\n",
    "        try:\n",
    "            cycle_start = datetime.now()\n",
    "            logger.info(f\"Starting trading cycle {self.cycle_count + 1}\")\n",
    "            \n",
    "            # Get current market data\n",
    "            market_data = self.get_current_market_data()\n",
    "            if not market_data:\n",
    "                raise Exception(\"No market data available\")\n",
    "            \n",
    "            current_price = market_data['price']\n",
    "            \n",
    "            # Get last price for comparison\n",
    "            last_price = None\n",
    "            if self.trades:\n",
    "                last_price = self.trades[-1]['price']\n",
    "            \n",
    "            actions_taken = []\n",
    "            \n",
    "            # Check DCA triggers\n",
    "            should_dca, dca_reason = self.should_execute_dca(market_data, last_price)\n",
    "            \n",
    "            if should_dca:\n",
    "                trade = self.execute_dca_buy(market_data, dca_reason)\n",
    "                if trade:\n",
    "                    actions_taken.append(f\"DCA buy: ${trade['usd_amount']:.2f}\")\n",
    "            \n",
    "            # Update portfolio value\n",
    "            portfolio_value = self.portfolio['USD'] + (self.portfolio['BTC'] * current_price)\n",
    "            self.bot_state['portfolio_value'] = portfolio_value\n",
    "            \n",
    "            # Update bot state\n",
    "            self.bot_state['last_cycle'] = cycle_start\n",
    "            self.bot_state['cycles_completed'] += 1\n",
    "            self.cycle_count += 1\n",
    "            \n",
    "            cycle_summary = {\n",
    "                'timestamp': cycle_start,\n",
    "                'cycle_number': self.cycle_count,\n",
    "                'current_price': current_price,\n",
    "                'actions_taken': actions_taken,\n",
    "                'portfolio_value': portfolio_value,\n",
    "                'usd_balance': self.portfolio['USD'],\n",
    "                'btc_balance': self.portfolio['BTC']\n",
    "            }\n",
    "            \n",
    "            logger.info(f\"Cycle {self.cycle_count} complete: {len(actions_taken)} actions\")\n",
    "            \n",
    "            return cycle_summary\n",
    "            \n",
    "        except Exception as e:\n",
    "            logger.error(f\"Trading cycle error: {e}\")\n",
    "            self.bot_state['errors_today'] += 1\n",
    "            self.error_count += 1\n",
    "            \n",
    "            # Send error alert if too many errors\n",
    "            if self.error_count >= 5:\n",
    "                self.whatsapp.send_trade_alert(\n",
    "                    trade_type=\"ERROR\",\n",
    "                    price=0,\n",
    "                    amount=0,\n",
    "                    reason=f\"Multiple errors: {str(e)[:100]}\"\n",
    "                )\n",
    "                self.error_count = 0  # Reset to prevent spam\n",
    "            \n",
    "            return None\n",
    "    \n",
    "    def run_bot(self, cycles_to_run=None):\n",
    "        \"\"\"\n",
    "        Run the trading bot for specified number of cycles or indefinitely\n",
    "        \"\"\"\n",
    "        try:\n",
    "            # Initialize components\n",
    "            if not self.initialize_trading_components():\n",
    "                logger.error(\"Failed to initialize trading components\")\n",
    "                return False\n",
    "            \n",
    "            self.running = True\n",
    "            self.bot_state['status'] = 'RUNNING'\n",
    "            \n",
    "            logger.info(f\"Bitcoin Trading Bot started - Target cycles: {cycles_to_run or 'Unlimited'}\")\n",
    "            \n",
    "            # Send startup notification\n",
    "            self.whatsapp.send_instant_message(\n",
    "                f\"üöÄ Bitcoin Trading Bot STARTED\\nüí∞ Budget: ${self.config['budget']:,.2f}\\nüìä DCA Trigger: {self.config['dca_percentage']}%\\n‚ö†Ô∏è ATR Multiplier: {self.config['atr_multiplier']}x\"\n",
    "            )\n",
    "            \n",
    "            cycle_count = 0\n",
    "            \n",
    "            while self.running:\n",
    "                if cycles_to_run and cycle_count >= cycles_to_run:\n",
    "                    logger.info(f\"Completed {cycles_to_run} cycles, stopping\")\n",
    "                    break\n",
    "                \n",
    "                # Execute trading cycle\n",
    "                cycle_result = self.execute_trading_cycle()\n",
    "                \n",
    "                if cycle_result:\n",
    "                    print(f\"\\nüìä Cycle {cycle_result['cycle_number']} Summary:\")\n",
    "                    print(f\"  Price: ${cycle_result['current_price']:,.2f}\")\n",
    "                    print(f\"  Actions: {len(cycle_result['actions_taken'])}\")\n",
    "                    for action in cycle_result['actions_taken']:\n",
    "                        print(f\"    ‚Ä¢ {action}\")\n",
    "                    print(f\"  Portfolio: ${cycle_result['portfolio_value']:,.2f}\")\n",
    "                    print(f\"  USD: ${cycle_result['usd_balance']:,.2f}\")\n",
    "                    print(f\"  BTC: {cycle_result['btc_balance']:.6f}\")\n",
    "                else:\n",
    "                    print(f\"‚ùå Cycle {self.cycle_count + 1} failed\")\n",
    "                \n",
    "                cycle_count += 1\n",
    "                \n",
    "                # Wait 30 minutes between cycles (for demo, we'll use 10 seconds)\n",
    "                if cycles_to_run != 1:  # Don't sleep if only running one cycle\n",
    "                    wait_time = 10 if cycles_to_run else 1800  # 10s for demo, 30min for production\n",
    "                    print(f\"‚è≥ Waiting {wait_time}s until next cycle...\")\n",
    "                    time.sleep(wait_time)\n",
    "            \n",
    "            self.bot_state['status'] = 'STOPPED'\n",
    "            logger.info(\"Trading bot stopped\")\n",
    "            \n",
    "            return True\n",
    "            \n",
    "        except KeyboardInterrupt:\n",
    "            logger.info(\"Bot stopped by user\")\n",
    "            self.running = False\n",
    "            return True\n",
    "        except Exception as e:\n",
    "            logger.error(f\"Bot error: {e}\")\n",
    "            self.bot_state['status'] = 'ERROR'\n",
    "            return False\n",
    "    \n",
    "    def get_bot_status(self):\n",
    "        \"\"\"\n",
    "        Get current bot status and statistics\n",
    "        \"\"\"\n",
    "        uptime = datetime.now() - self.start_time\n",
    "        \n",
    "        return {\n",
    "            'status': self.bot_state['status'],\n",
    "            'uptime': str(uptime).split('.')[0],  # Remove microseconds\n",
    "            'cycles_completed': self.bot_state['cycles_completed'],\n",
    "            'trades_today': self.bot_state['trades_today'],\n",
    "            'errors_today': self.bot_state['errors_today'],\n",
    "            'portfolio_value': self.bot_state['portfolio_value'],\n",
    "            'last_cycle': self.bot_state['last_cycle'],\n",
    "            'total_trades': len(self.trades) if hasattr(self, 'trades') else 0,\n",
    "            'usd_balance': self.portfolio.get('USD', 0) if hasattr(self, 'portfolio') else 0,\n",
    "            'btc_balance': self.portfolio.get('BTC', 0) if hasattr(self, 'portfolio') else 0\n",
    "        }\n",
    "\n",
    "# Initialize trading bot\n",
    "trading_bot = BitcoinTradingBot()\n",
    "\n",
    "print(\"\\nü§ñ Bitcoin Trading Bot Initialized\")\n",
    "print(f\"  Status: {trading_bot.bot_state['status']}\")\n",
    "print(f\"  Notifications: WhatsApp ‚úÖ, Email {'‚úÖ' if gmail.configured else '‚ùå'}\")\n",
    "print(f\"  Ready for deployment: ‚úÖ\")"
