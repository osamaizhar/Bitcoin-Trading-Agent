    "# WhatsApp notification class using PyWhatKit\n",
    "class WhatsAppNotifier:\n",
    "    def __init__(self, phone_number=\"+923353015576\"):\n",
    "        self.phone_number = phone_number\n",
    "        self.last_message_time = None\n",
    "        self.message_queue = []\n",
    "        \n",
    "    def send_instant_message(self, message, wait_time=10):\n",
    "        \"\"\"\n",
    "        Send WhatsApp message instantly using PyWhatKit\n",
    "        \"\"\"\n",
    "        try:\n",
    "            import pywhatkit as kit\n",
    "            \n",
    "            # Prevent spam - wait at least 30 seconds between messages\n",
    "            if self.last_message_time:\n",
    "                time_diff = datetime.now() - self.last_message_time\n",
    "                if time_diff.total_seconds() < 30:\n",
    "                    print(f\"‚è≥ Rate limiting: waiting {30 - time_diff.total_seconds():.0f}s\")\n",
    "                    time.sleep(30 - time_diff.total_seconds())\n",
    "            \n",
    "            # Format message with timestamp\n",
    "            formatted_message = f\"ü§ñ BTC Bot Alert\\n{datetime.now().strftime('%H:%M:%S')}\\n\\n{message}\"\n",
    "            \n",
    "            # Send message instantly\n",
    "            kit.sendwhatmsg_instantly(\n",
    "                phone_no=self.phone_number,\n",
    "                message=formatted_message,\n",
    "                wait_time=wait_time,\n",
    "                tab_close=True,\n",
    "                close_time=2\n",
    "            )\n",
    "            \n",
    "            self.last_message_time = datetime.now()\n",
    "            logger.info(f\"WhatsApp message sent to {self.phone_number}\")\n",
    "            print(f\"üì± WhatsApp sent: {message[:50]}...\")\n",
    "            return True\n",
    "            \n",
    "        except ImportError:\n",
    "            print(\"‚ùå pywhatkit not installed - WhatsApp disabled\")\n",
    "            return False\n",
    "        except Exception as e:\n",
    "            logger.error(f\"WhatsApp error: {e}\")\n",
    "            print(f\"‚ùå WhatsApp failed: {str(e)}\")\n",
    "            return False\n",
    "    \n",
    "    def send_trade_alert(self, trade_type, price, amount, reason, pnl=None):\n",
    "        \"\"\"\n",
    "        Send formatted trade alert\n",
    "        \"\"\"\n",
    "        if trade_type == \"DCA_BUY\":\n",
    "            message = f\"üü¢ DCA BUY EXECUTED\\nüí∞ ${amount:.2f} ‚Üí {amount/price:.6f} BTC\\nüìä Price: ${price:,.2f}\\nüìù Reason: {reason}\"\n",
    "        elif trade_type == \"STOP_LOSS\":\n",
    "            pnl_emoji = \"üìà\" if pnl > 0 else \"üìâ\"\n",
    "            message = f\"üî¥ STOP-LOSS TRIGGERED\\nüí∏ Sold at ${price:,.2f}\\n{pnl_emoji} P&L: ${pnl:+.2f}\\nüìù {reason}\"\n",
    "        elif trade_type == \"ERROR\":\n",
    "            message = f\"‚ö†Ô∏è SYSTEM ERROR\\nüö® {reason}\\nüîß Check logs for details\"\n",
    "        else:\n",
    "            message = f\"üìä {trade_type}\\nüí∞ ${price:,.2f}\\nüìù {reason}\"\n",
    "        \n",
    "        return self.send_instant_message(message)\n",
    "    \n",
    "    def send_daily_summary(self, portfolio_value, day_pnl, trades_count):\n",
    "        \"\"\"\n",
    "        Send daily portfolio summary\n",
    "        \"\"\"\n",
    "        pnl_emoji = \"üìà\" if day_pnl >= 0 else \"üìâ\"\n",
    "        message = f\"üìä DAILY SUMMARY\\nüíº Portfolio: ${portfolio_value:,.2f}\\n{pnl_emoji} Day P&L: ${day_pnl:+.2f}\\n‚ö° Trades: {trades_count}\"\n",
    "        return self.send_instant_message(message)\n",
    "\n",
