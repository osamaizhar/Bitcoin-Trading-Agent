ments.txt .\n\n# Install Python dependencies\nRUN pip install --no-cache-dir -r requirements.txt\n\n# Copy application\nCOPY . .\n\n# Create data directory\nRUN mkdir -p data logs\n\n# Expose port (if needed for web interface)\nEXPOSE 8080\n\n# Run bot\nCMD [\"python\", \"-c\", \"from notebooks.bot_main import run_production_bot; run_production_bot()\"]\n\"\"\"\n\nwith open('../Dockerfile', 'w') as f:\n    f.write(dockerfile_content)\n\n# Create docker-compose.yml\ndocker_compose_content = \"\"\"\nversion: '3.8'\n\nservices:\n  btc-trading-bot:\n    build: .\n    container_name: btc-trading-bot\n    restart: unless-stopped\n    volumes:\n      - ./data:/app/data\n      - ./logs:/app/logs\n      - ./.env:/app/.env:ro\n    environment:\n      - PYTHONUNBUFFERED=1\n    networks:\n      - btc-bot-network\n\nnetworks:\n  btc-bot-network:\n    driver: bridge\n\"\"\"\n\nwith open('../docker-compose.yml', 'w') as f:\n    f.write(docker_compose_content)\n\nprint(\"\\nüöÄ DEPLOYMENT PREPARATION COMPLETE\")\nprint(\"=\" * 45)\nprint(\"\\nüìÅ Files Created:\")\ndeployment_files = ['deploy.sh', 'Dockerfile', 'docker-compose.yml']\nfor file in deployment_files:\n    file_path = f\"../{file}\"\n    exists = os.path.exists(file_path)\n    print(f\"  {file}: {'‚úÖ' if exists else '‚ùå'}\")\n\nprint(\"\\nüîß Deployment Options:\")\nprint(\"  1. üñ•Ô∏è  Local Deployment: ./deploy.sh\")\nprint(\"  2. üê≥ Docker: docker-compose up -d\")\nprint(\"  3. ‚òÅÔ∏è  Cloud: Deploy to DigitalOcean/AWS with Docker\")\n\nprint(\"\\n‚ö†Ô∏è Before Deployment:\")\nprint(\"  ‚Ä¢ Update .env with real API keys\")\nprint(\"  ‚Ä¢ Test with paper trading first\")\nprint(\"  ‚Ä¢ Set up WhatsApp Web on deployment server\")\nprint(\"  ‚Ä¢ Configure Gmail app password\")\nprint(\"  ‚Ä¢ Test all notifications\")\n\nprint(\"\\nüìä Monitoring:\")\nprint(\"  ‚Ä¢ Logs: tail -f data/bot.log\")\nprint(\"  ‚Ä¢ Status: Check WhatsApp messages\")\nprint(\"  ‚Ä¢ Reports: Monday 9 AM email reports\")"
